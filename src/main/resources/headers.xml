<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!-- @formatter:off -->
<properties>
	<entry key="ACTION">Action</entry>
	<entry key="DAYS">d</entry>
	<!-- BLACKCRAZE: Concerning both "DEVICE" and "LANGUAGE": Since `bot userconfig set` needs the English words to work
	correctly, I will modify the non-English files to show the English word followed by the non-English word in
	parentheses. That will give those users both the needed word for the command and the understanding of what it is. -->
	<entry key="DEVICE">device: %s [</entry>
	<entry key="GROUP_LIST_HEADER">List of Resource Groups</entry>
	<entry key="HOURS">h</entry>
	<entry key="LANGUAGE">language: %s%n</entry>
	<entry key="MINUTES">m</entry>
	<entry key="NAME">Name</entry>
	<entry key="OLDEST_STOCK">Age</entry>
	<entry key="POPULATED">Populated</entry>
	<entry key="QUANTITY">Quantity</entry>
	<entry key="RAW_MATERIAL">Resource</entry>
	<entry key="SECONDS">s</entry>
	<entry key="TOTAL_RESOURCES">Total Guild Resources</entry>
	<entry key="UPDATED">Updated</entry>
	<entry key="USER">User</entry>
	<entry key="USERS_LIST_HEADER">Guild members (%d)</entry>
	<entry key="USERS_SYNC_HEADER">User synchronization</entry>
	<entry key="WEEKS">w</entry>
	<!-- -->
	<!-- MESSAGES SHOWN MOSTLY (BUT NOT EXCLUSIVELY) TO DEVELOPERS APPEAR BELOW -->
	<!-- Headers for command "nativeStatus" -->
	<entry key="JAVACPP">JAVACPP:%n</entry>
	<entry key="DEALLOCATORS">Memory tracked by deallocators: </entry>
	<entry key="MAX_MEM_TRACKED">%nMaximum memory allowed to be tracked: </entry>
	<entry key="PHYS_MEM_INSTALLED">Physical memory installed, as reported by operating system (0 if unknown): </entry>
	<entry key="MAX_PHYS_MEM">%nMaximum physical memory that should be used: </entry>
	<entry key="PHYS_MEM_FREE">%nPhysical memory free, as reported by operating system (0 if unknown): </entry>
	<entry key="PHYS_MEM_WHOLE">%nPhysical memory currently used by whole process (0 if unknown): </entry>
	<!-- Headers for command "status" -->
	<entry key="MY_MEM">My Memory:%n%n</entry>
	<entry key="INIT_HEAD">%n%tINIT:      </entry>
	<entry key="USED">%n%tUSED:      </entry>
	<entry key="COMMITTED">%n%tCOMMITTED: </entry>
	<entry key="MAX">%n%tMAX:       </entry>
	<!-- I'm not actually sure the two below can be used at all, even if you keep all this crap. LOL -->
	<entry key="HEAP">Heap</entry>
	<entry key="NON-HEAP">Non-Heap</entry>
</properties>
<!-- @formatter:on -->
